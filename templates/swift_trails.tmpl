import SwiftUI
import MapKit

final class {{name}}: Trail {
    init() {
        super.init(title: "{{name}}", sections: [
            {{- range .}}
            Section(title: "{{.Name}}", coordinates: [
            {{- range .TrackPoints}}
                CLLocationCoordinate2D(latitude: {{ .Latitude }}, longitude: {{.Longitude -}}),
            {{- end}}
            ]),
            {{- end}}
        ])
    }
}
